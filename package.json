{
  "name": "shortlink-backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "build": "npx tsc && npm run sentry:sourcemaps",
    "start": "dotenvx run -- node dist/main.js",
    "test": "dotenv -e .env.test -- jest",
    "test:watch": "dotenv -e .env.test -- jest --watch",
    "test:coverage": "dotenv -e .env.test -- jest --coverage",
    "dev": "concurrently \"npx tsc --watch\" npm run sentry:sourcemaps \"dotenvx run -- nodemon -q dist/main.js\"",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org arfandy --project node-express ./dist && sentry-cli sourcemaps upload --org arfandy --project node-express ./dist",
    "db:test:reset": "dotenv -e .env.test -- npx prisma db push --force-reset",
    "db:test:seed": "dotenv -e .env.test -- ts-node prisma/seed.ts"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@dotenvx/dotenvx": "^1.9.0",
    "@prisma/client": "^6.5.0",
    "@sentry/cli": "^2.33.1",
    "@sentry/node": "^9.12.0",
    "@sentry/profiling-node": "^9.12.0",
    "@types/cookie-parser": "^1.4.8",
    "@types/supertest": "^6.0.2",
    "axios": "^1.9.0",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "cron": "^4.1.4",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.5.0",
    "gelf-pro": "^1.3.12",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "supertest": "^7.0.0",
    "uuid": "^10.0.0",
    "winston": "^3.14.2",
    "zod": "^3.23.8",
    "lusca": "^1.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/preset-typescript": "^7.24.7",
    "@jest/globals": "^29.7.0",
